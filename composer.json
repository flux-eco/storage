{
  "name": "flux-eco/storage",
  "description": "Handling mysql databases with json based table schemas",
  "version": "1.0.0",
  "keywords": [
    "flux-eco",
    "storage",
    "fluxlabs"
  ],
  "homepage": "https://fluxlabs.ch",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs ag",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@fluxlabs.ch"
  },
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-yaml": "*",
    "flux-eco/dot-env": ">=0.0.1",
    "laminas/laminas-code": ">=4.4",
    "laminas/laminas-db": ">=2.12"
  },
  "require-dev": {
  },
  "autoload": {
    "files": [
      "fn/appendData.php",
      "fn/countTotalRows.php",
      "fn/createStorage.php",
      "fn/deleteData.php",
      "fn/deleteStorage.php",
      "fn/getData.php",
      "fn/storeData.php"
    ],
    "psr-4": {
      "FluxEco\\Storage\\": [
        "src/"
      ],
      "fluxStorage\\": [
        "fn/"
      ]
    }
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform-check": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  }
}
